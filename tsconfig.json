{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": "src",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "paths": {
            "*": [
                "node_modules/*"
            ]
        }
    },
    "include": [
        "src/**/*.ts", "src/infra/BDD.js"
, "src/infra/api/controllers/auth.controller.ts.old"    ],
    "exclude": [
        "node_modules",
        ".vscode"
    ]
}
